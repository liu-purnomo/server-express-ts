{
  "name": "remote-pilot-indonesia",
  "version": "1.0.0",
  "description": "server untuk portal remote pilot indonesia",
  "main": "app.ts",
  "scripts": {
    "build": "tsc && cp -r src/config dist/",
    "dev": "nodemon ./src/app.ts",
    "start": "node ./dist/app",
    "db:create": "cd src && sequelize db:drop && sequelize db:create && sequelize db:migrate && cd .."
